{
    "name": "goszowski/laravel-database-log-channel",
    "description": "Laravel database log channel",
    "keywords": [
        "goszowski",
        "laravel",
        "laravel-database-log-channel"
    ],
    "homepage": "https://github.com/goszowski/laravel-database-log-channel",
    "license": "MIT",
    "authors": [
        {
            "name": ":goszowski",
            "email": "goszowski@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Goszowski\\DatabaseLogChannel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Goszowski\\DatabaseLogChannel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goszowski\\DatabaseLogChannel\\DatabaseLogChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
